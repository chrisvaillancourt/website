{
  "name": "chris-vaillancourt-website",
  "version": "0.0.1",
  "description": "Chris Vaillancourt's personal website.",
  "type": "module",
  "scripts": {
    "dev": "astro check --watch & astro dev --host",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && pnpm lint && astro build",
    "wrangler": "wrangler",
    "deploy": "wrangler pages deploy dist",
    "preview": "astro preview",
    "preview:wrangler": "wrangler pages dev ./dist",
    "astro": "astro",
    "tsc": "tsc",
    "format": "prettier --write --ignore-path .gitignore --plugin-search-dir=. .",
    "lint": "eslint src/**/*.{ts,astro} --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^1.0.3",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.1",
    "postcss": "^8.4.24",
    "postcss-custom-media": "^9.1.4",
    "postcss-jit-props": "^1.0.13",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "typescript": "^5.1.3",
    "wrangler": "^3.1.0"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "astro": "^2.6.4",
    "open-props": "^1.5.8"
  }
}