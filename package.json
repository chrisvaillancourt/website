{
  "name": "christopher-vaillancourt-website",
  "version": "0.0.1",
  "description": "Christopher Vaillancourt's personal website.",
  "type": "module",
  "scripts": {
    "dev": "astro check --watch & astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "tsc": "tsc",
    "format": "prettier --write --ignore-path .gitignore --plugin-search-dir=. .",
    "lint": "eslint src/**/*.{ts,astro} --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "astro": "^2.1.4",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.26.0",
    "open-props": "^1.5.6",
    "postcss": "^8.4.21",
    "postcss-jit-props": "^1.0.12",
    "prettier": "2.8.6",
    "prettier-plugin-astro": "^0.8.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96"
  }
}