{
  "name": "christopher-vaillancourt-website",
  "version": "0.0.1",
  "description": "Christopher Vaillancourt's personal website.",
  "type": "module",
  "scripts": {
    "dev": "astro check --watch & astro dev --host",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && pnpm lint && astro build",
    "wrangler": "wrangler",
    "deploy": "wrangler pages publish dist",
    "preview": "astro preview",
    "preview:wrangler": "wrangler pages dev ./dist",
    "astro": "astro",
    "tsc": "tsc",
    "format": "prettier --write --ignore-path .gitignore --plugin-search-dir=. .",
    "lint": "eslint src/**/*.{ts,astro} --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^1.0.3",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "astro": "^2.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.26.1",
    "open-props": "^1.5.7",
    "postcss": "^8.4.21",
    "postcss-custom-media": "^9.1.3",
    "postcss-jit-props": "^1.0.13",
    "prettier": "2.8.7",
    "prettier-plugin-astro": "^0.8.0",
    "typescript": "^5.0.4",
    "wrangler": "^2.15.1"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96"
  }
}