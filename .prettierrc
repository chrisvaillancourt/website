{
	"plugins": [
		"./node_modules/prettier-plugin-astro",
		"./node_modules/prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	],
	"bracketSameLine": false,
	"proseWrap": "always",
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all"
}
