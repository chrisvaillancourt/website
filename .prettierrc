{
  "plugins": [
    "./node_modules/prettier-plugin-astro",
    "./node_modules/prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": false,
  "trailingComma": "all",
  "proseWrap": "always",
  "endOfLine": "lf"
}